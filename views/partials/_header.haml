%header#header
	%h1
		%a{ :href => "/" }= get_info['site_title']
	%nav
		%ul
			%li
				%a{ :href => "/" } Index
			%li
				%a{ :href => "/information" } Information
			- if get_info['twitter']
				%li.facebook
					%a{ :href => "http://twitter.com/#{get_info['twitter']}" } Twitter
			- if get_info['github']
				%li.github
					%a{ :href => "http://github.com/#{get_info['github']}" } Github